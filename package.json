    "@codemirror/view": "^6.38.7",
    "@headlessui/react": "^1.7.17",
    "@uiw/react-codemirror": "^4.21.21",
    "dexie": "^3.2.4",
    "diff-match-patch": "^1.0.5",
    "file-saver": "^2.0.5",
    "framer-motion": "^10.16.16",
    "js-sha256": "^0.10.1",
    "jszip": "^3.10.1",
    "nanoid": "^5.1.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.1",
