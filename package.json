{
  "name": "prompt-studio",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "description": "AI 提示词版本管理与编辑工具",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
